{
    "modes": {
        "css": {
            "name": "CSS",
            "highlighter": "ace/mode/css",
            "extensions": ["css"],

            "commands": {
                "Tools:Beautify": {
                    "scriptUrl": "./beautify.js",
                    "inputs": {
                        "preferences": true
                    }
                },
                "Tools:Check": {
                    "scriptUrl": "./check.js",
                    "options": {},
                    "inputs": {
                        "text": true
                    },
                    "internal": true
                },
                "Tools:CTags": {
                    "scriptUrl": "./ctags.js",
                    "inputs": {
                        "text": true
                    },
                    "internal": true
                }
            },
            "handlers": {
                "check": ["Tools:Check"],
                "change": ["Tools:CTags"]
            }
        }
    }
}
